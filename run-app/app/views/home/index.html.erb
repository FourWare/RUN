            </div>
        </div>
        <br><br/>
        <div class="row">
            <div class="col-sm-3">
                <div class="team-member">
                    <h4>Miguel Angel Borja</h4>
                    <%= image_tag("team/borja.png", width: '150', height: '150', alt: 'Miguel Angel Borja', class: "mx-auto img-circle") %>
                    <br><br/>
                    <h5 class="text-muted">ScrumMaster</h5>
                </div>
            </div>
            <div class="col-sm-3">
                <div class="team-member">
                    <h4>Camilo Rueda</h4>
                    <%= image_tag("team/rueda.png", width: '150', height: '150', alt: 'Camilo Rueda', class: "mx-auto img-circle") %>
                    <br><br/>
                    <p class="text-muted">Desarrollador</p>
                </div>
            </div>
            <div class="col-sm-3">
                <div class="team-member">
                    <h4>Santiago Marquez</h4>
                    <%= image_tag("team/smo.jpg", width: '150', height: '150', alt: 'Santiago Márquez', class: "mx-auto img-circle") %>
                    <br><br/>
                    <p class="text-muted">Desarrollador</p>
                </div>
            </div>
            <div class="col-sm-3 text-center">
                <div class="team-member">
                    <h4>Miguel Villamil</h4>
                    <%= image_tag("team/villamil.jpg", width: '150', height: '150', alt: 'Miguel Villamil', class: "mx-auto img-circle") %>
                    <br></br>
                    <p class="text-muted">Desarrollador</p>
                </div>
            </div>
        </div>
    </div>
</section>


<!-- Contact -->
    <section class="content-section-b" id="contact">
      <div class="container">
        <div class="row">
          <div class="col-lg-12 text-center">
            <h2 class="section-heading">Contáctenos</h2>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <form id="contactForm" name="sentMessage" novalidate>
              <div class="row">
                <div class="col-md-6">
                  <div class="form-group">
                    <input class="form-control" id="nombre" style="background-color: #e7e7e7;" type="text" placeholder="Nombre *" required data-validation-required-message="Por favor ingrese su nombre.">
                    <p class="help-block text-danger"></p>
                  </div>
                  <div class="form-group">
                    <input class="form-control" id="email" style="background-color: #e7e7e7;" type="email" placeholder="Email *" required data-validation-required-message="Por favor ingrese su email.">
                    <p class="help-block text-danger"></p>
                  </div>
                  <div class="form-group">
                    <input class="form-control" id="telefono" style="background-color: #e7e7e7;" type="tel" placeholder="Telefono *" required data-validation-required-message="Por favor ingrese su numero de telefono.">
                    <p class="help-block text-danger"></p>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group">
                    <textarea class="form-control" id="mensaje" style="background-color: #e7e7e7;" placeholder="Ingrese su mensaje *" required data-validation-required-message="Por favor ingrese su mensaje."></textarea>
                    <p class="help-block text-danger"></p>
                  </div>
                </div>
                <div class="clearfix"></div>
                <div class="col-lg-12 text-center">
                  <div id="success"></div>
                  <button id="sendMessageButton" class="btn btn-xl" type="submit">Enviar</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </section>


<!-- /.content-section-a -->
<aside class="banner">
